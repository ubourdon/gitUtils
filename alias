#Alias maven
alias mci='mvn clean install -T4'
alias mcit='mvn clean install -T4 -Dmaven.test.skip'

#Alias git
alias glog='git log --color --oneline --decorate --graph'
alias gst='git status'
alias gdiff='git diff --color'
alias gco='git checkout'
alias gshow='git show --name-only'
alias gstandup='git log --since yesterday --oneline --author' #rajouter l'auteur Ã  la suite de la commande